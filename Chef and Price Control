Cpp code :

#include<bits/stdc++.h>
#include <iostream>

using namespace std;
typedef long long ll;
int main()
{
    ll t;
    cin>>t;
    while(t--)
    {
        ll n,k,i,j,s;
        s=0;
        j=0;
        cin>>n>>k;
        ll a[n];
        for(i=0;i<n;i++)
        {
            cin>>a[i];
            j=j+a[i];
            if(a[i]>k)
            {
                a[i]=k;
            }
            s=s+a[i];
        }
        
        cout<<(j-s)<<endl;
        
    }
    return 0;
}
